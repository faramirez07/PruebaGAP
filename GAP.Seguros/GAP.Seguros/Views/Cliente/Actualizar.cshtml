@model GAP.Seguros.ModeloDatos.Cliente
@{
    ViewBag.Title = "Actualizar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Actualizar Cliente</h2>

<form method="post" action="@Url.Action("Actualizar","Cliente")">
    <div class="form-group" style="visibility:hidden">
        @Html.TextBoxFor(x => x.IdCliente, new { @class = "form-control" })
    </div>
    <div class="form-group">
        <label>Cédula</label>
        @Html.TextBoxFor(x => x.Cedula, new { @class = "form-control" })
    </div>
    <div class="form-group">
        <label>Nombre</label>
        @Html.TextBoxFor(x => x.Nombre, new { @class = "form-control" })
    </div>
    <div class="form-group">
        <label>Edad</label>
        @Html.TextBoxFor(x => x.Edad, new { @class = "form-control" })
    </div>
    <div class="form-group">
        <label>Nacionalidad</label>
        @Html.TextBoxFor(x => x.Nacionalidad, new { @class = "form-control" })
    </div>
    <div class="form-group">
        <label>Estado</label>
        @Html.CheckBoxFor(model => model.Activo)
    </div>
    <div class="form-group">
        <label>Usuario Creación</label>
        @Html.TextBoxFor(x => x.UsuarioCreacion, new { @class = "form-control" })
    </div>
    <div class="form-group">
        <label>Fecha Creación</label>
        @Html.TextBoxFor(x => x.FechaCreacion, new { @class = "form-control" })
    </div>
    <div class="form-group">
        <input type="submit" class="btn-success" value="Enviar" />
        <a href="@Url.Action("Index", "Cliente")" class="btn btn-info riht">Regresar</a>
    </div>


</form>




